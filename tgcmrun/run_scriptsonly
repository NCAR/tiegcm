#!/bin/tcsh
#
# Make namelist and job scripts (do not compile or submit runs):
#
#==========================================================================================
set stdout_dir = benchmark_scripts_res5.0

 tgcmrun -run_number 0 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 1 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
