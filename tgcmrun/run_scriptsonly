 tgcmrun -run_number 9 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 10 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 11 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 12 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 13 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 14 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir
 tgcmrun -run_number 16 -model_name tiegcm -model_res 5.0 -submit no -stdout_dir $stdout_dir

